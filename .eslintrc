{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "jasmine": true
  },
  "plugins": [
    "react"
  ],
  "extends": [
    "plugin:react/recommended"
  ],
  "globals": {
    "module": false,
    "require": false
  },
  "rules": {
    "block-scoped-var": 2,
    "brace-style": [2, "1tbs", { "allowSingleLine": true }],
    "camelcase": 0,
    "comma-style": [2, "last"],
    "comma-dangle": ["error", "never"],
    "consistent-this": [2, "self"],
    "constructor-super": 2,
    "default-case": 2,
    "dot-notation": 2,
    "func-style": [2, "expression"],
    "guard-for-in": 2,
    "keyword-spacing": 2,
    "no-case-declarations": 2,
    "no-class-assign": 2,
    "no-compare-neg-zero": 2,
    "no-cond-assign": 2,
    "no-const-assign": 2,
    "no-constant-condition": 2,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-delete-var": 2,
    "no-dupe-args": 2,
    "no-dupe-class-members": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-else-return": 2,
    "no-empty": 2,
    "no-empty-character-class": 2,
    "no-empty-pattern": 2,
    "no-eq-null": 2,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-semi": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 2,
    "no-func-assign": 2,
    "no-global-assign": 2,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-lonely-if": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-multiple-empty-lines": 2,
    "no-new-symbol": 2,
    "no-obj-calls": 2,
    "no-octal": 2,
    "no-redeclare": 2,
    "no-regex-spaces": 2,
    "no-self-assign": 2,
    "no-self-compare": 2,
    "no-sparse-arrays": 2,
    "no-this-before-super": 2,
    "no-undef": 2,
    "no-underscore-dangle": 0,
    "no-unexpected-multiline": 2,
    "no-unreachable": 2,
    "no-unsafe-finally": 2,
    "no-unsafe-negation": 2,
    "no-unused-labels": 2,
    "no-unused-vars": [2, {"vars": "all", "args": "none"}],
    "no-useless-escape": 2,
    "no-warning-comments": 1,
    # "one-var": [2, "always"],
    "operator-assignment": [2, "always"],
    "quote-props": 0,
    "quotes": [2, "single"],
    "radix": 2,
    "require-yield": 2,
    "semi": [2, "always"],
    "use-isnan": 2,
    "valid-jsdoc": [0, { "prefer": { "returns": "return" }, "requireReturn": false }],
    "valid-typeof": 2,
    "wrap-iife": 2
  }
}
